<section class="index_header equipment-header">
<h1>Equipment</h1>
</section>

<div class="all_items">
    #foreach ($e in $equipment)
    <div class="single_item">
        <p>$e.getDescription()</p>
        <p>$e.getPrice()</p>
        <a href="/equipment/$e.getId()">Show More</a>
    </div>
    #end
</div>