<section class="index_header equipment-header">
    <h1>Coffee</h1>
</section>
<div class="all_items">
    #foreach ($c in $coffee)
    <div class="single_item" style="background-image: url($c.getImage())">
        <p>$c.getName()</p>
        <p>$c.getPrice()</p>
        <a href="/coffee/$c.getId()">Show More</a>
    </div>
    #end
</div>